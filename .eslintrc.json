{
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "eslint-plugin-jest"
    ],
    "env": {
        "jest/globals": true
    },
    "rules": {
        "no-console": 0,
        "max-len": [1, 120],
        "padded-blocks": 0,
        "no-param-reassign": "off",
        "no-shadow": "off",
        "object-curly-newline": ["error", { "consistent": true }]
    }
}
